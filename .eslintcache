rt({ id, title, price, description })\n      );\n    }).catch((err) =>{\n      console.error(err);\n    })\n  };\n\n  return (\n    <li className={classes.item}>\n      <Card>\n        <header>\n          <h3>{title}</h3>\n          <div className={classes.price}>${price.toFixed(2)}</div>\n        </header>\n        <p>{description}</p>\n        <div className={classes.actions}>\n          <button onClick={() => filterAddOrUpdate(props)}>Add to Cart</button>\n        </div>\n      </Card>\n    </li>\n  );\n};\n\nexport default ProductItem;\n","C:\\Users\\a.rivas.jimenez\\Documents\\React\\new one\\01-starting-project\\02-starting-project\\src\\components\\Layout\\MainHeader.js",[],"C:\\Users\\a.rivas.jimenez\\Documents\\React\\new one\\01-starting-project\\02-starting-project\\src\\components\\UI\\Card.js",[],"C:\\Users\\a.rivas.jimenez\\Documents\\React\\new one\\01-starting-project\\02-starting-project\\src\\components\\Cart\\CartButton.js",[],"C:\\Users\\a.rivas.jimenez\\Documents\\React\\new one\\01-starting-project\\02-starting-project\\src\\store\\index.js",[],"C:\\Users\\a.rivas.jimenez\\Documents\\React\\new one\\01-starting-project\\02-starting-project\\src\\reducer\\initialsStates.js",[],"C:\\Users\\a.rivas.jimenez\\Documents\\React\\new one\\01-starting-project\\02-starting-project\\src\\reducer\\uiSlices.js",[],"C:\\Users\\a.rivas.jimenez\\Documents\\React\\new one\\01-starting-project\\02-starting-project\\src\\actions\\uiAction.js",[],"C:\\Users\\a.rivas.jimenez\\Documents\\React\\new one\\01-starting-project\\02-starting-project\\src\\reducer\\cartSlice.js",[],"C:\\Users\\a.rivas.jimenez\\Documents\\React\\new one\\01-starting-project\\02-starting-project\\src\\actions\\cardActions.js",[],"C:\\Users\\a.rivas.jimenez\\Documents\\React\\new one\\01-starting-project\\02-starting-project\\src\\reducer\\productsSlice.js",[],"C:\\Users\\a.rivas.jimenez\\Documents\\React\\new one\\01-starting-project\\02-starting-project\\src\\api\\connectToBD.js",[],"C:\\Us